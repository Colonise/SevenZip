    "bugs": {
        "url": "https://github.com/Colonise/SevenZip/issues"
    },
    "homepage": "https://github.com/Colonise/SevenZip#readme",
    "devDependencies": {
        "@types/del": "^3.0.1",
        "@types/gulp": "^4.0.5",
        "@types/node": "^10.1.2",
        "alsatian": "^2.2.1",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^4.0.2",
        "tsgulp": "^1.1.2",
        "tslint": "^5.10.0",
        "typescript": "^2.7.2"
    },
    "dependencies": {
